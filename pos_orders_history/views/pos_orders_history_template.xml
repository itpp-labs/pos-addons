<odoo>
    <template
        id="assets_backend"
        name="pos_orders_history assets"
        inherit_id="point_of_sale.assets"
    >
        <xpath expr="." position="inside">
            <script
                type="text/javascript"
                src="/pos_orders_history/static/src/js/db.js"
            />
            <script
                type="text/javascript"
                src="/pos_orders_history/static/src/js/models.js"
            />
            <script
                type="text/javascript"
                src="/pos_orders_history/static/src/js/screens.js"
            />
            <script
                type="text/javascript"
                src="/pos_orders_history/static/lib/js/JsBarcode.all.js"
            />
            <link rel="stylesheet" href="/pos_orders_history/static/src/css/pos.css" />
        </xpath>
    </template>

    <template
        id="demo_assets_backend"
        name="pos_orders_history demo assets"
        inherit_id="web.assets_backend"
    >
        <xpath expr="." position="inside">
            <script
                type="text/javascript"
                src="/pos_orders_history/static/src/js/tour.js"
            />
        </xpath>
    </template>
</odoo>
