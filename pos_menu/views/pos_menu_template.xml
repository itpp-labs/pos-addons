<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template
        id="point_of_sale_assets"
        name="pos_menu assets"
        inherit_id="point_of_sale.assets"
    >
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/pos_menu/static/src/js/pos.js" />
        </xpath>
    </template>

    <template
        id="demo_assets_backend"
        name="pos_menu demo assets"
        inherit_id="web.assets_backend"
    >
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/pos_menu/static/src/js/tour.js" />
        </xpath>
    </template>
</odoo>
