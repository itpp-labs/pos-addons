<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template
        id="assets_backend"
        name="pos_employee_select assets"
        inherit_id="point_of_sale.assets"
    >
        <xpath expr="." position="inside">
            <script
                type="text/javascript"
                src="/pos_employee_select/static/src/js/models.js"
            />
            <script
                type="text/javascript"
                src="/pos_employee_select/static/src/js/screens.js"
            />
            <script
                type="text/javascript"
                src="/pos_employee_select/static/src/js/gui.js"
            />
            <script
                type="text/javascript"
                src="/pos_employee_select/static/src/js/chrome.js"
            />
            <link
                rel="stylesheet"
                href="/pos_employee_select/static/src/css/styles.css"
            />
        </xpath>
    </template>
</odoo>
