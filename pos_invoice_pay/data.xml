<odoo>
    <template
        id="assets_backend"
        name="pos_invoice_pay"
        inherit_id="point_of_sale.assets"
    >
        <xpath expr="." position="inside">
            <link rel="stylesheet" href="/pos_invoice_pay/static/src/css/styles.css" />
            <script
                type="text/javascript"
                src="/pos_invoice_pay/static/src/js/main.js"
            />
