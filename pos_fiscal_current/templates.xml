<?xml version="1.0" encoding="utf-8" ?>
<openerp>
    <data>
        <!-- POS -->
        <template
            id="assets_backend"
            name="pos_fiscal_current assets"
            inherit_id="point_of_sale.assets"
        >
            <xpath expr="." position="inside">
                <script
                    type="text/javascript"
                    src="/pos_fiscal_current/static/src/js/pos.js"
                />
            </xpath>
            <xpath expr="//link[@id='pos-stylesheet']" position="after">
              <link
                    rel="stylesheet"
                    href="/pos_fiscal_current/static/src/css/pos_fc.css"
                />
            </xpath>
        </template>
    </data>
</openerp>
